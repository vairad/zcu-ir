<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Separator?>

<fx:root type="javafx.scene.layout.VBox" xmlns:fx="http://javafx.com/fxml"
         alignment="CENTER"
         maxHeight="-Infinity"
         maxWidth="-Infinity"
         minHeight="-Infinity"
         minWidth="-Infinity"
         spacing="2.0">

        <Label fx:id="documentName"   styleClass="title"/>
        <Label fx:id="presentQuery" styleClass="presentWords"/>
        <Label fx:id="missingQuery" styleClass="missingWords"/>
        <!--        <Label fx:id="preview" styleClass="preview"/> -->
        <Separator />
</fx:root>